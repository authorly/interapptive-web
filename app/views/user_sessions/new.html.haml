= form_tag sign_in_path, :class => 'well' do |f|
  %h1 Sign in to Interapptive
  %br/

  %p
    = text_field_tag :email, nil, :placeholder => 'email'
  %p
    = password_field_tag :password, nil, :placeholder => 'password'
  
  = submit_tag 'Sign In', :class => 'btn'

  %br/
  %br/

  = link_to 'Forgot your password?', new_password_reset_path
